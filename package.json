{
  "name": "kimi-assistant",
  "version": "2.0.0",
  "description": "Assistente sempre vis√≠vel - Modernizado com Vite + React",
  "main": "electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run web:dev\" \"wait-on http://localhost:5173 && cross-env NODE_OPTIONS='--dns-result-order=ipv4first' electron .\"",
    "build": "npm run build && electron-builder",
    "web:dev": "vite",
    "web:build": "vite build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/genai": "^1.40.0",
    "@google/generative-ai": "^0.24.1",
    "@lancedb/lancedb": "^0.24.1",
    "@xenova/transformers": "^2.17.2",
    "dotenv": "^17.2.4",
    "framer-motion": "^12.33.0",
    "node-fetch": "^3.3.2",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "zod": "^4.3.6",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "vite": "^5.4.2",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.kimi.assistant",
    "productName": "Kimi Assistant",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "win": {
      "target": [
        "nsis"
      ]
    }
  }
}
